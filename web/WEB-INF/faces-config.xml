    <!--**************** navigation configurations *******************************-->



    <!--core SYSTEM entry pathway-->
    <!--we need users to pass through this cycle as soon as they try to access anything in /restricted-->
    
    <navigation-rule>
        <from-view-id>/public/systemHome.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>publicPortal</from-outcome>
            <to-view-id>/public/publicDashboard.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>startInitiationProcess</from-outcome>
            <to-view-id>/restricted/initiateInternalSession.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>systemHome</from-outcome>
            <to-view-id>/public/systemHome.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>


    <navigation-rule>
        <from-view-id>/restricted/initiateInternalSession.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/restricted/missionControl.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/restricted/error.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        
    </navigation-rule>
    
    <!--Public-->

    <navigation-rule>
        <from-view-id>/public/submitCERequest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/public/actionReqSubConf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>

        <from-view-id>*</from-view-id>    
        
        <navigation-case>
            <from-outcome>codeEnforce</from-outcome>
            <to-view-id>/public/services/submitCERequest.xhtml</to-view-id>
            <redirect />
        </navigation-case>

    </navigation-rule>
        
    <!--site-wide navigation-->    


    
    <navigation-rule>
